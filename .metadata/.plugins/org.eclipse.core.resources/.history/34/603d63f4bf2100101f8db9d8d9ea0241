package practice01;

public class Test1 {

	public static int[] test(String[] strlist) {
		int[] total = new int[strlist.length];

		for (int i = 0; i < strlist.length; i++) {
			total[i] = strlist[i].length();
		}
		return total;
	}

	public static void main(String[] args) {
		String[] words = { "We", "are", "learining", "codiingJava" };
		int[] result = test(words);
		for (int i=0; i<words.length; i++) {
			System.out.println(words[i]+result[i]);
		}
	}
}
