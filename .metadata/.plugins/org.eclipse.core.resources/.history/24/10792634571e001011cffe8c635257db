package chapter06;

public class Vending {

	private Can[] can = new Can[5];
	private int money;

	// 생성자

	// 메소드
	public void init() {
		can[0] = new Can("Fanta", 1000);
		can[1] = new Can("Sprite", 1200);
		can[2] = new Can("Orangeju", 1100);
		can[3] = new Can("BananaMi", 1400);
		can[4] = new Can("Cola", 1000);
	}

}
