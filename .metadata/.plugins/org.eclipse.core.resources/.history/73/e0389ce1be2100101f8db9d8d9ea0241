package practice01;

public class Test1 {

	public static int[] test(String[] strlist) {
		int[] answer=new int[strlist.length];
		
		for(int i=0; i<strlist.length; i++) {
			answer[i]=strlist[i].length();
		}
		return answer;
	}
	 public static void main(String[] args) {
		 String[] strlist= {"apple", "banana","kiwi"};
		 int[] result=Test1(strlist);
	 for(int len:result) {
		 System.out.println(len);
	 }
	 }
}
