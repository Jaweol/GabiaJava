package chapter13;

public class GeneralNoGeneric {

	// 멤버변수
	// 생성자
	// 메서드
	// 정수 배열 출력 메서드
	public void printIntArr(Integer[] arr) {
		for (int i = 0; i < arr.length; i++) {
			System.out.println(" " + arr[i]);
		}
		System.out.println();
	}

	public void printDoubleArr(Integer[] arr) {
		for (int i = 0; i < arr.length; i++) {
			System.out.println(" " + arr[i]);
		}
		System.out.println();
	}

	public void printCharArr(Integer[] arr) {
		for (int i = 0; i < arr.length; i++) {
			System.out.println(" " + arr[i]);
		}
		System.out.println();
	}
}
