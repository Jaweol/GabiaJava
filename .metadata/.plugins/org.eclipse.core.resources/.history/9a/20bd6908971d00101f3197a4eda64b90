package chapter06;

public class CarMain {

	public static void main(String[] args) {

		Car_04 obj = new Car_04();

		System.out.println(obj.company);
		obj.company = "현대";
		System.out.println(obj.company);
		obj.model = "k5";
		obj.color = "흰색";
		obj.maxSpeed = 280;
		obj.speed = 0;

		System.out.println();

	}

}
