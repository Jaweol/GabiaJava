package practice01;

public class Test3 {

	public static void printScores() {
		// 배열 초기화

		System.out.println(score);
	}

	// main 메서드
	public static void main(String[] args) {

		int[] scores = { 90, 80, 70, 100 };

		// 배열의 값을 하나씩 출력
		for (int score : scores) {

			printScores(); // 값을 전달하지 않고 메서드를 호출
		}
	}
}
