package chapter12.clone;

public class Cloaneable {

	Point point;
	int radius;

	public class Circle(Point point, int radius) {
		this.point=point;
		this.radius=radius;
	}

	@Override
	protected Object clone() throws CloneNotSupportedException {

		return super.clone();
	}

	@Override
	public String toString() {

		return super.toString();
	}

}
