package practice01;

public class Test1 {

	public static int arrayname(String[] strlist) {
		int[] total = new int[strlist.length];

		for (int i = 0; i < strlist.length; i++) {
			total[i] = strlist[i].length();
		}
		return total;

	}

	public static void main(String[] args) {

		String[]words= {"we","are","learning","Coding"};
		int[] result=arrayname(words);
		
		for(int i=0; i<words.length;i++) {
			System.out.println(words[i]+result[i]);
		}
	}
}
